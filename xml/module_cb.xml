function checkInputs(noAnswerRow) {
	var baseId = "ans$(this.uid)\\.0\\.";
	var maxRow = noAnswerRow ? ${len(this.rows) - 1} : ${len(this.rows)};
	
	for (var i = 0; i &lt; maxRow; i++) {
		var current = $ ("#" + baseId + i)
		var next = $ ("#" + baseId + (i + 1))
		var nextElement = $ ("div.element").eq(i + 1)
		
		if (current.val().trim() !== ''){
			next.parent().fadeIn(400);
		    nextElement.fadeIn(400);
		} else {
		    next.val('')
		    next.parent().fadeOut(400);
		    nextElement.fadeOut(400);
		}
	}
}
